{
  "name": "phpixie/project",
  "type": "project",
  "description": "PHPixie skeleton project",
  "license": "BSD",
  "autoload": {
    "psr-4": {
      "": [
        "src/",
        "tests/",
        "bundles/app/src/",
        "bundles/cp/src/",
        "bundles/api/src/"
      ]
    }
  },
  "require": {
    "phpixie/bundles": "~3.2",
    "phpixie/framework": "~3.6",

    "phpixie/default-bundle": "~3.3",
    "phpixie/bundle-framework": "~3.8",
    "phpixie/framework-bundle": "~3.12",

    "mthaml/mthaml": "^1.8",

    "tedivm/stash": "^0.14.1",

    "monolog/monolog": "^1.21",

    "ezyang/htmlpurifier": "~4.8",

    "ircmaxell/random-lib": "^1.2",

    "ralouphie/mimey": "^1.0",

    "swiftmailer/swiftmailer": "^5.4",
    "openbuildings/swiftmailer-css-inliner": "^0.4.0",

    "bshaffer/oauth2-server-php": "~1.8",

    "curl/curl": "^1.5",

    "nesbot/carbon": "^1.21",
    "deimos/imaginarium-sdk": "^0.0",
    "deimos/helper": "^0.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.*-dev"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php ./console framework:generateBundle app",
      "php ./console framework:installWebAssets"
    ]
  }
}
