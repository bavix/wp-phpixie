.panel-body
  %form(method="POST" data-id="#{$brand->id}" action="/api/soc/brand/#{$brand->id}/heading" data-created="heading")

    .grid
      .col-md-12
        .form-group.form-group-lg
          %label.control-label(for="headingType") Type
          %select#headingType.form-control.select2-multiple(name="id" style="width: 100%")
            %option(disabled selected) None

    .hr-line-dashed

    .form-group(align="center")
      %button.btn.btn-primary.btn-lg
        %i.fa.fa-save
        Submit

  // http://wbs-cms/api/soc/heading?terms[brands.id]=9
  #headingRows.table-responsive